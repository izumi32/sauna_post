<% if logged_in? %>
  <div class="col-md-8">
    <h3>投稿一覧</h3>
    <% if @feed_items.any? %>
      <ol class="microposts">
        <%= render @feed_items %>
      </ol>
      <%= will_paginate @feed_items %>
    <% end %>
  </div>
<% else %>
  <h1>サウナシェア</h1>
  <p>ホーム画面</p>
  <%= link_to "新規登録", signup_path, class: "btn btn-primary" %>
<% end %>
